{{#if commitLog.length}}
    <div class="row-fluid">
        <div class="span12">
            <div class="extend results" id="history_log_results">
                {{#each commitLog}}
                    <article>
                        <date>{{this.text2}}</date> {{this.text}}
                    </article>
                {{/each}}
            </div>
        </div>
    </div>
{{/if}}
