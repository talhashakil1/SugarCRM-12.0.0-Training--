{{#if usersPanel}}
    <div class="{{usersPanel.css_class}}">
        {{partial this.usersTemplate this view=this}}
    </div>
{{/if}}
{{#if headerPanel}}
    <div class="{{headerPanel.css_class}}">
        <span class="header-update-text">
            {{str "LBL_UPDATE_TO_MODULE" ../module}}
            {{getModuleName ../updateModule}}
        </span>
        {{#each headerPanel.fields}}
            {{field ../this model=../activity.attributes}}
        {{/each}}
    </div>
{{/if}}
