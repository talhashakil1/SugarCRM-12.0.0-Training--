<div class="change-container">
    {{#each changeDef}}
        <div class="{{css_class}}-container{{#eq type "enum-colorcoded"}}-pill{{/eq}}">
            <span class="{{css_class}}">
                {{#if name}}
                    {{field ../../view model=../model template=../template}}
                {{else}}
                    {{str 'LBL_BLANK' ../../view.module}}
                {{/if}}
            </span>
        </div>
    {{/each}}
</div>
