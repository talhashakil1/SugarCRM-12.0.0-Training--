<div class="row-fluid">
    <div class="span12">
        <!--Module List-->
        <div class="row-fluid panel_body">
            <div class="span12 record-cell">
                <select class="select2 dm-tag-builder-modules dm-select" placeholder="{{str 'LBL_DOCUMENT_MERGE_SELECT_MODULE' module}}">
                    <option></option>
                    {{#each modules}}
                        <option value={{moduleName}}>{{moduleLabel}}</option>
                    {{/each}}
                </select>
            </div>
        </div>
    </div>
</div>
