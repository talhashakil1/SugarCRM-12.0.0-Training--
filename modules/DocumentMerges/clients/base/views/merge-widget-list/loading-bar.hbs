<div class="row-fluid">
    <span class="text">
        {{#eq model.status "success"}}
            <a target="_blank"
                href="#Documents/{{model.generated_document_id}}">{{str "LBL_GENERATED_DOCUMENT" "DocumentMerges"}}</a>
            {{else}}
                {{model.message}}
        {{/eq}}
    </span>
</div>
<div class="row-fluid">
    <div
        class="progress {{#eq model.status "success"}}progress-success{{/eq}} {{#eq model.status "error"}}progress-danger{{/eq}}">
        <div class="bar" data-merge-id={{model.id}}></div>
    </div>
</div>
