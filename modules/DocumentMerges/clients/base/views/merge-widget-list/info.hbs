<div class="row-fluid">
    <div>
        <span class="dm-bold">{{model.name}}</span>
    </div>
</div>
<div class="row-fluid">
    <div class="span2">
        <span class="label label-module label-module-sm label-{{model.parent_type}}" rel="tooltip"
            data-title="{{model.parent_type}}" data-original-title=""
            title="">{{moduleIconLabel model.parent_type}}</span>
    </div>
    <div class="dm-left span10">
        {{#if model.isMultiMerge}}
            <span>{{str 'LBL_DOCUMENTMERGE_MULTIPLE' 'DocumentMerges'}}</span>
        {{else}}
            <a href="#{{model.parent_type}}/{{model.parent_id}}">{{model.parent_name}}</a>
        {{/if}}
    </div>
</div>
