<div class="headerpane">
    <h1>
    <span class="record-cell" data-type="dashboardtitle">
        <span sfuuid="493" class="detail">
            <div>
                {{#eq preview_type 'tile'}}
                    {{str 'LBL_PIPELINE_TILE_PREVIEW' this.module}}
                {{else}}
                    {{str 'LBL_PIPELINE_HEADER_PREVIEW' this.module}}
                {{/eq}}
            </div>
        </span>
    </span>
    </h1>
</div>
<div class="dashboard pipeline-records">
    <div class="cols row-fluid">
        <ul class="dashlets row-fluid">
            <li class="span12">
                <div class="tile portlet">
                    <div class="span12 ">
                        {{#with previewModel}}
                            <div class="tile_header portlet-header">
                                <div class="name ellipsis_inline pull-left" data-placement="bottom" title=""
                                     data-original-title="{{str tile_header moduleName}}">
                                    <a href="javascript:void(0)">
                                        { {{str tile_header moduleName}} }
                                    </a>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="tile_body">
                                <div class="info-container span12">
                                    {{#each tile_body_fields}}
                                        <span sfuuid="606" class="list">
                                <div class="ellipsis_inline" data-placement="bottom" title=""
                                     data-original-title="{{str this ../moduleName}}">
                                    { {{str this ../moduleName}} }
                                </div>
                            </span>
                                    {{/each}}
                                </div>
                            </div>
                        {{/with}}
                    </div>
                    <div class="clearfix"></div>
                </div>
            </li>
        </ul>
    </div>
</div>
