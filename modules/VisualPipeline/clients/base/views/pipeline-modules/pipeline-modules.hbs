<div class="pipeline-modules">
    <div class="accordion-heading active">
        <span class="pull-left step-circle pipeline-step-circle">
            <span>1</span>
            <i class="sicon sicon-lg sicon-check-circle-lg"></i>
        </span>
        <h4>{{str "LBL_PIPELINE_GENERAL_SETTINGS" this.module}}</h4>
    </div>
    <div class="accordion-inner">
        {{#each meta.panels}}
            {{#each fields}}
                <div class="row-fluid">
                    <div class="record-label" data-name="{{name}}">
                        {{#if label}}
                            {{str label ../../../this.module}}
                        {{/if}}
                    </div>
                    {{field ../../this model=../../this.model template="edit"}}
                </div>
            {{/each}}
        {{/each}}
</div>
