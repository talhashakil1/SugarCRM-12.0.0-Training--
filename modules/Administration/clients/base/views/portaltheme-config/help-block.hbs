<div class="admin-config-help-block">
    <ul>
        {{#each this}}
            {{#if this.is_link}}
                <li {{#if this.css_class}}class="{{this.css_class}}"{{/if}} data-name="{{this.name}}">
                    <a href="{{this.link_href}}"
                       class="{{this.link_css_class}}"
                       target="{{this.link_target}}"
                       role="button">{{this.link_text}}</a> {{this.text}}
                </li>
            {{else}}
                <li class="{{this.label}}"><b>{{this.name}}</b> {{this.text}}</li>
            {{/if}}
        {{/each}}
    </ul>
</div>
