<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str "LBL_ACTIONBUTTON_MODULE"}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit inherit-width" data-fieldname="module">
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str 'LBL_ACTIONBUTTON_LINK'}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit">
                    <input type="checkbox" data-fieldname="must-link-record" {{#if
                        _properties.mustLinkRecord}}checked{{/if}} />
                    <a class="inherit-width {{#notEq _properties.mustLinkRecord true}}hidden{{/notEq}}" data-fieldname="link"></a>
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str 'LBL_ACTIONBUTTON_AUTO_CREATE'}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit">
                    <input type="checkbox" data-fieldname="auto-create" {{#if _properties.autoCreate}}checked{{/if}} />
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str 'LBL_ACTIONBUTTON_COPY_PARENT'}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit">
                    <input type="checkbox" data-fieldname="copy-from-parent" {{#if
                        _properties.copyFromParent}}checked{{/if}} />
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str "LBL_ACTIONBUTTON_PRESET"}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit inherit-width" data-fieldname="attributes">
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span6 record-cell edit" data-type="text">
    <div class="labels-on-side">
        <div class="record-label-wrapper span2">
            <div class="record-label">
                {{str "LBL_ACTIONBUTTON_PRESET_PARENT"}}
            </div>
        </div>
        <div class="record-link-wrapper span4">
            <span class="normal index">
                <span class="edit inherit-width" data-fieldname="parent-attributes">
                </span>
            </span>
        </div>
    </div>
</div>
<div class="span12 ab-config-column-stretch" data-container="preset-fields" style="">
</div>
