<div class="tab-pane fade active in">
    <div class="row-fluid">
        <div class="span12 padded" data-type="title">
            <h4 class="tbold">{{this.options.widgetModule}} {{str 'LBL_MAPS_MODULE_OPTIONS' module}}</h4>
        </div>
        <div class="span8 record-cell edit">
            <span class="detail">
                <div class="labels-on-side">
                    <div class="record-label-wrapper span4">
                        <div class="ellipsis_inline record-label">
                            <span class="text-overflow">{{str 'LBL_MAPS_AUTOPOPULATE_CITY_CODE' module}}</span>
                        </div>
                    </div>
                    <div class="record-link-wrapper span1">
                        <span class="normal index">
                            <span class="edit">
                                <select class="select2 inherit-width" data-fieldname="autopopulate">
                                    {{#each _settings.autopopulate}}
                                            <option value={{@key}}>{{str this}}</option>
                                    {{/each}}
                                </select>
                            </span>
                        </span>
                    </div>
                </div>
            </span>
        </div>
    </div>
</div>
