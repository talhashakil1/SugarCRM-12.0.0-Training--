<a class="sicon sicon-close pull-right feedback-close" data-action="close"></a>
{{#each meta.panels}}
    <p>{{str label ../module}}</p>
    {{#each fields}}
        <div class="row-fluid control-group">
            <h5>{{str label ../../module}}</h5>
            <div class="controls span12">
                {{field ../../this model=../../model template=template}}
            </div>
        </div>
    {{/each}}
{{/each}}
<div class="row-fluid feedback-footer">
    <span class="span12">
        <button class="btn btn-primary pull-right span12" data-action="submit">{{str 'LBL_FEEDBACK_BTN_SUBMIT' module}}</button>
    </span>
</div>
<aside>{{aside}}</aside>
