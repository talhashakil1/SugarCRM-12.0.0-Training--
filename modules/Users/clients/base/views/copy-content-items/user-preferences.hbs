{{#with templateComponent}}
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="default_teams" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_DEFAULT_TEAMS' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="favorite_reports" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_FAVORITE_REPORTS' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="navigation_bar" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_NAVBAR_SELECTION' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="notify_on_assignment" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_NOTIFY_ON_ASSIGNMENT' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="reminder_options" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_REMINDER_OPTIONS' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="scheduled_reporting" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_SCHEDULED_REPORTING' module}}</span>
    </div>
    <div class="row-fluid mb-2.5">
        <input type="checkbox" name="sugar_email_client" class="mx-2"/> <span class="text-sm absolute ml-0.5">{{str 'LBL_SUGAR_EMAIL_CLIENT' module}}</span>
    </div>
{{/with}}
