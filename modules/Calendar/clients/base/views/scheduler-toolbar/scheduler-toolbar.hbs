<span class='k-scheduler-navigation k-button-group'>
    <button class='k-button k-button-icon k-icon-button k-nav-prev' title="{{str 'LBL_CALENDAR_PREVIOUS' 'Calendar'}}" aria-label="{{str 'LBL_CALENDAR_PREVIOUS' 'Calendar'}}"><span class='sicon sicon-chevron-left'></span></button>
    <button class='k-button k-nav-today' title="{{str 'LBL_CALENDAR_TODAY' 'Calendar'}}"><span class='k-button-text'>{{str 'LBL_CALENDAR_TODAY' 'Calendar'}}</span></button>
    <button class='k-button k-button-icon k-icon-button k-nav-next' title="{{str 'LBL_CALENDAR_NEXT' 'Calendar'}}" aria-label="{{str 'LBL_CALENDAR_NEXT' 'Calendar'}}"><span class='sicon sicon-chevron-right'></span></button>
</span>
<a role='button' href='' class='k-nav-current'><span class='calendarIcon sicon sicon-lg sicon-calendar-lg'></span><span class='k-sm-date-format'>{{formattedShortDate}}</span><span class='k-lg-date-format'>{{formattedDate}}</span></a><span class='k-spacer k-toolbar-spacer'></span>
<span class='k-scheduler-views-wrapper'>
    <span class='k-scheduler-views k-button-group force-display'>
        <select class='k-dropdown k-views-dropdown'>
        {{#each views}}
            <option value='{{type}}' {{#if selected}}selected{{/if}}>{{title}}</option>
        {{/each}}
        </select>
    </span>
</span>
<span class='k-button-group'>
    <div class='dropdown usersLegend'>
        <button class='k-button k-button-icon k-icon-button dropdown-toggle' data-toggle='dropdown'>
            <span>{{str 'LBL_USERS' 'Calendar'}}</span><i class='sicon sicon-chevron-down'></i>
        </button>
        <ul class='dropdown-menu'></ul>
    </div>
</span>
<span class='k-button-group'>
    <div class='dropdown exportButtons'>
        <button class='k-button k-button-icon k-icon-button dropdown-toggle' data-toggle='dropdown'>
            <i class='shareButton'></i>
        </button>
        <ul class='dropdown-menu'>
            <li><div role='button' class='k-button k-pdf'>{{str 'LBL_CALENDAR_PDF_EXPORT' 'Calendar'}}</div></li>
            <li><div role='button' class='k-button export_icalendar'>{{str 'LBL_CALENDAR_EXPORT_ICAL' 'Calendar'}}</div></li>
            <li><div role='button' class='k-button publish_icalendar'>{{str 'LBL_CALENDAR_PUBLISH_ICAL' 'Calendar'}}</div></li>
        </ul>
    </div>
</span>
<span class='k-button-group'>
    <div class='userSettings'>
        <button class='k-button k-button-icon k-icon-button dropdown-toggle'>
            <i class='sicon sicon-settings'></i>
        </button>
    </div>
</span>
