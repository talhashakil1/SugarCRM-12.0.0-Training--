<nav>
    <ul id='columns-sortable' class='field-list connectedSortable' module_name="{{model.fieldModule}}">
        {{#each selectedFields}}
            <li class='pill outer' rel='tooltip'
                fieldname="{{name}}" fieldlabel="{{label}}"
                data-original-title="{{str label ../model.fieldModule}}">
                {{str label ../model.fieldModule}}
                <i class="sicon sicon-remove console-field-remove"></i>
            </li>
        {{/each}}
    </ul>
