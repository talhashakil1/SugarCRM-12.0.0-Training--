{{#with view}}
    <div class="header-user-to-user">
        {{partial 'user' this view=this userField=leftUserField model=activity email_addr=this.from_email}}
        <span class="arrow">
            <i class="sicon sicon-arrow-{{#if isRtl}}left{{else}}right{{/if}}"></i>
        </span>
        {{partial 'user' this view=this userField=rightUserField model=activity email_addr=this.to_email}}
    </div>
{{/with}}
