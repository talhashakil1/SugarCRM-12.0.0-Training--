{{#if showProcesses}}
    <div class="control-group dashlet-options">
        <div class="row-fluid">
            <div class="span12">{{#each dashletConfig.processes_selector}}
                {{field ../this model=../this.settings template="edit"}}
            {{/each}}</div>
        </div>
    </div>
{{/if}}
<div class="process-users-chart">
    <div class="sc-chart" data-content="chart">
        <svg id="{{cid}}"></svg>
    </div>
    <div class="block-footer hide" data-content="nodata">{{str "LBL_NO_DATA_AVAILABLE"}}</div>
</div>
