<tr name="{{model.module}}_{{model.cid}}" class="flex-list-row-header quote-data-group-header ui-not-droppable">
    <td class="two-dropdowns"{{#if name}} data-fieldname="{{name}}"{{/if}}>
        <span class="action-button-wrapper left-column-save-cancel">
            {{#each leftSaveCancelColumn}}
                {{field ../this model=../model template=../viewName}}
            {{/each}}
        </span>
        <span class="action-button-wrapper left-column-buttons">
            {{#each leftColumns}}
                {{field ../this model=../model template=../viewName}}
            {{/each}}
        </span>
    </td>
    {{#each _fields}}
        <td{{#if align}} class="{{align}}"{{/if}} data-type="{{type}}" colspan="{{../listColSpan}}">
            <div class="quote-data-group-title">
                <span class="group-loading-icon">
                    <i class="sicon sicon-refresh sicon-is-spinning"></i>
                </span>
                {{field ../this model=../model template=../viewName}}
            </div>
        </td>
    {{/each}}
</tr>
