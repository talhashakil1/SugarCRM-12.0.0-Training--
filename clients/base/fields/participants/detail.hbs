{{#if model.attributes.invitees.isLoading}}
    {{str "LBL_LOADING" module}}
{{else}}
<div class="participants-schedule">
    <div class="row header">
        <div class="cell"></div>
        <div class="cell times" data-render="timeline-header"></div>
        <div class="cell"></div>
    </div>
    <div class="row spacer">
        <div class="cell"></div>
    </div>
    {{#each value}}
    <div class="row participant" data-module="{{module}}" data-id="{{id}}">
        <div class="cell profile">
            <div class="container">
                <div class="avatar">
                    {{#if avatar}}
                    <img class="avatar" src="{{avatar}}" alt="{{name}}" />
                    {{else}}
                    <span class="label label-module label-{{module}}" rel="tooltip" title="{{moduleIconToolTip module}}">
                        {{moduleIconLabel module}}
                    </span>
                    {{/if}}
                </div>
                <div class="name">
                    {{#if isNameErased}}
                        {{partial 'erased' ../../this this fallbackTemplate='erased'}}
                    {{else}}
                        <span class="ellipsis_inline" title="{{name}}">{{name}}</span>
                    {{/if}}
                    {{#unless email}}
                        <span class="flag error">
                            <i class="sicon sicon-close"></i><i class="sicon sicon-email"></i>
                        </span>
                    {{/unless}}
                </div>
                <div class="status">
                    <div class="label{{#if accept_status.css_class}} label-{{accept_status.css_class}}{{/if}}">
                        {{str accept_status.label ../module}}
                    </div>
                </div>
            </div>
        </div>
        <div class="cell times{{#notEq module 'Users'}} unavailable{{/notEq}}">
            <div data-toggle="loading" class="overlay hide">{{str "LBL_LOADING"}}</div>
            {{#notEq module 'Users'}}
                <div class="overlay">
                    {{str "LBL_CALENDAR_EVENT_SCHEDULE_UNAVAILABLE" ../../module}}
                </div>
            {{/notEq}}
            {{#eq module 'Users'}}
                <div class="start_end_overlay"></div>
            {{/eq}}
            <div class="container">
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <div class="timeblock">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>
        <div class="cell buttons">
            <div class="btn-toolbar pull-right">
                <button type="button" class="btn{{#unless preview.enabled}} disabled{{/unless}}" rel="tooltip" title="{{str preview.label}}" data-action="previewRow" data-module="{{module}}" data-id="{{id}}">
                    <i class="sicon sicon-preview"></i>
                </button>
            </div>
        </div>
    </div>
    <div class="row spacer">
        <div class="cell"></div>
    </div>
    {{/each}}
</div>
<button class="btn btn-link btn-invisible more" data-action="show-more">{{showMoreTemplate}}</button>
{{/if}}
