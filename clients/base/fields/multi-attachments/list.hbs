<div class="ellipsis_inline"
    data-placement="bottom"
    title="{{fileList}}"
    data-container="body"
    rel="tooltip">
    {{#each value}}
        <a class="ellipsis-inline"
            href="{{url}}"
            data-url="{{url}}"
            target="_blank">{{fileName}}{{fileExt}}
        </a>{{#unless @last}}, {{/unless}}
    {{/each}}
</div>
