<div class='preview-data'>
    <div class='record-cell field-row'>
        {{#each keys}}
            <div class='stage2-notification-key'>
                <div class='stage2-notification-key-left'>
                    <div class='stage2-notification-icon fa-stack'>
                        <i class='fa fa-stack-1x fa-{{icon}} {{disabledClass}}'></i>
                        {{#if iconStack}}
                            <span class='fa-stack-1x {{disabledClass}} text-fa'>{{iconStack}}</span>
                        {{/if}}
                    </div>
                </div>
                <div class='stage2-notification-key-right'>
                    {{#if title}}
                        <div class='stage2-notification-key-title'>
                            {{title}}
                        </div>
                    {{/if}}
                    {{#if details}}
                        <div class='stage2-notification-key-text'>
                            {{details}}
                        </div>
                    {{/if}}
                </div>
            </div>
        {{/each}}
    </div>
</div>
