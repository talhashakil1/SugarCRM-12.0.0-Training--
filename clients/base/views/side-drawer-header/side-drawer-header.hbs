{{#if display}}
<div class='headerpane'>
    <h1>
        <span class='record-cell' data-type="{{type}}" data-name="{{name}}">
            <span class='detail'>
                <div class='ellipsis_inline'>
                    {{str 'LBL_DASHBOARD_CONFIG'}}
                </div>
            </span>
        </span>
        <div class='btn-toolbar'>
            {{#each meta.buttons}}
                {{field ../this model=../this.model template="detail"}}
            {{/each}}
        </div>
    </h1>
</div>
{{/if}}
