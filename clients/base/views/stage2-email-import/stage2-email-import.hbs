<div class="modal hide quick-create stage2-email-import">
    <div class="modal-header">
        <h4>{{str LBL_HINT_EMAIL_IMPORT_TITLE}}</h4>
    </div>
    <div class="modal-body">
        <div class="panel-import">
            <ul class="email-list">
                {{#each _emailCollection}}
                <li class="item">
                    <div>
                        <input type="checkbox" name="checkbox-import" value="{{address}}"/>
                        <span class="detail">{{address}}</span>
                    </div>
                </li>
                {{/each}}
            </ul>
            <div class="modal-footer">
                <button class="btn cancel pull-right" data-name="email-import-btn" >{{str 'LBL_CANCEL_BUTTON_LABEL'}}</button>
                <button class="btn save_import pull-left" data-name="email-import-btn">{{str 'LBL_CONFIRM_BUTTON_LABEL'}}</button>
            </div>
        </div>
    </div>
</div>
