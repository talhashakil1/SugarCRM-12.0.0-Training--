<ul class="omnichannel-search-facet-list">
    {{#each facets}}
        <li class="omnichannel-search-facet-list-item" data-facet-id="{{facetId}}" data-facet-criteria-id="{{facetCriteriaId}}">
            <span class="ellipsis_inline">
                <input type="checkbox" class="omnichannel-search-facet-select"{{#if selected}} checked{{/if}}{{#unless count}} disabled{{/unless}}>
                <span class="omnichannel-search-facet-label">{{label}} ({{count}})</span>
            </span>
        </li>
    {{/each}}
</ul>
